stlNames ( "genSphere0" "genSphere1" "genSphere2" "genSphere3" "genSphere4" "genSphere5" "genSphere6" );

interpolationSchemes
{
  U cellPointFace;
}

surfaceThreshold    1e-4;
minDEMloops    5;
maxDistInDEMloop    0.00005;

wallProps
{
	kN     5e-3;
	gammaN 1e-6;
	kt     1e-1;
	gammat 1e-7;
	mu 1e-1;
	adhN 1;
}

